class Solution {
    public boolean queryString(String S, int N) {
       for(int i=0;i<=N;i++){
           StringBuilder sb=new StringBuilder();
           int temp=i;
           while(temp>0){
           //通过N/2取余的形式逐位将N转变成String
               sb.append(temp%2);
               temp/=2;
           }
           if(!S.contains(sb.reverse().toString()))
               return false;
       }
        return true;        
    }
}
